{
  "name": "split-well-informed",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "server": "tsx src/index.ts",
    "server:dev": "tsx watch src/index.ts",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:start": "docker compose start",
    "db:stop": "docker compose stop",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test:db:up": "docker compose -f compose.test.yml up -d",
    "test:db:down": "docker compose -f compose.test.yml down",
    "pretest": "npm run test:db:up && npx wait-port 5433 && dotenv -e .env.test -- npx prisma db push",
    "test": "dotenv -e .env.test -- vitest run ",
    "posttest": "npm run test:db:down"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@prisma/client": "^6.19.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
