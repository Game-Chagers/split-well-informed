    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.2",
    "@types/react": "~19.1.0",
    "@types/supertest": "^6.0.3",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2",
